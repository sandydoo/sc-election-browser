{
  "name": "sc-election-browser",
  "private": true,
  "workspaces": [
    "app/*",
    "packages/*"
  ],
  "scripts": {
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "fetch": "pnpm --filter @sc-election/fetcher -w start",
    "fetch:candidates": "pnpm --filter @sc-election/fetcher -w start fetch-candidates",
    "fetch:questions": "pnpm --filter @sc-election/fetcher -w start fetch-questions",
    "fetch:responses": "pnpm --filter @sc-election/fetcher -w start fetch-responses",
    "fetch:all": "pnpm --filter @sc-election/fetcher -w start fetch-all"
  },
  "devDependencies": {
    "drizzle-kit": "^0.31.4"
  },
  "dependencies": {
    "@eslint/css": "^0.11.1",
    "@eslint/js": "^9.37.0",
    "@eslint/json": "^0.13.2",
    "@eslint/markdown": "^7.3.0",
    "eslint": "^9.37.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "typescript-eslint": "^8.45.0"
  }
}
